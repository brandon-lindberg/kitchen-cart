<%# Map for the food carts %>
<%= form_tag food_carts_path, method: :get do %>
  <%= text_field_tag :query,
    params[:query],
    class: "form-control",
    placeholder: "Find a Food Cart"
  %>
  <%= submit_tag "Search", class: "btn btn-primary" %>
<% end %>
<%= render partial: 'shared/react_entry', locals: {data: {user: current_user, votes: @votes, food_carts: @food_carts, markers: @markers, schedules: @schedules}.to_json, content_type: "FOOD_CARTS"} %>